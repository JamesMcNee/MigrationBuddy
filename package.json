{
  "name": "migrationbuddy",
  "version": "0.1.0",
  "description": "Utility to aid in the migration of endpoints (especially useful for cloud migrations)",
  "main": "./lib/index.js",
  "bin": {
    "migbuddy": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p .",
    "install:local": "npm run build && (cd lib && npm link)",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "keywords": [
    "migration",
    "utility",
    "cloud",
    "endpoint"
  ],
  "author": "James McNee",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "cli-progress": "^3.8.2",
    "commander": "^7.2.0",
    "figlet": "^1.5.0",
    "json-diff": "0.5.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
