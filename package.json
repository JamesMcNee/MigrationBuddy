{
  "name": "migrationbuddy",
  "version": "4.4.0",
  "description": "Utility to aid in the migration of endpoints (especially useful for cloud migrations)",
  "main": "./index.js",
  "bin": {
    "migbuddy": "./index.js",
    "migrationbuddy": "./index.js"
  },
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./lib && tsc -p . && cp -r src/resources ./lib/ && cp package*.json ./lib/ && cp README.md ./lib/",
    "install:local": "sudo npm rm migrationbuddy -g && npm run build && (cd lib && sudo npm link)",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "keywords": [
    "migration",
    "utility",
    "cloud",
    "endpoint"
  ],
  "author": "James McNee",
  "homepage": "https://github.com/JamesMcNee/MigrationBuddy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesMcNee/MigrationBuddy.git"
  },
  "license": "Unlicence",
  "dependencies": {
    "ajv": "^8.0.5",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "cli-progress": "^3.8.2",
    "clipboardy": "^2.3.0",
    "commander": "^7.2.0",
    "figlet": "^1.5.0",
    "handlebars": "^4.7.7",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^26.0.22",
    "@types/json-diff": "^0.5.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^14.14.37",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
